        }
    ],
    "require": {
        "php": ">=5.4",
        "zfr/zfr-stripe": "~3.0",
        "zendframework/zend-modulemanager": "^2.8",
        "zendframework/zend-servicemanager": "^3.0"
    },
    "require-dev": {
